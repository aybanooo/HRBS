<IfModule mod_rewrite.c>
    RewriteEngine On
    RewriteBase /admin/
    #RewriteRule ^.*$ htaccess_tester.php

    #RewriteCond %{REQUEST_FILENAME} !-f

    #nireredirect lahat ng url na may .html papunta sa root
    #RewriteCond %{THE_REQUEST} \.html
    #RewriteRule ^(.*)\.html$ / [R=301,L]

    #RewriteCond %{REQUEST_FILENAME} !-f

    #RewriteRule ^sad$ admin-webAnalytics.html [R=302]

    #custom Urls
    RewriteRule ^analytics$ admin-webAnalytics.html [NC,L]
    RewriteRule ^rooms$ admin-page-room.html [NC,L]
    RewriteRule ^amenities$ admin-page-amenities.html [NC,L]
    RewriteRule ^accounts$ admin-accounts.html [NC,L]
    RewriteRule ^vouchers$ admin-vouchers.php [NC,L]
    RewriteRule ^billing$ admin-billing.html [NC,L]
    RewriteRule ^settings-system$ admin-settings-system.php [NC,L]
    RewriteRule ^appearance$ admin-settings-appearance.html [NC,L]
    RewriteRule ^webpage$ admin-settings-webpage.html [NC,L]
    RewriteRule ^profile$ admin-settings-myAccount.html [NC,L]

    RewriteRule ^rooms/(.*)$ customFiles/php/database/roomControls/displayRoom.php?roomTypeID=$1 [NC,L]
    RewriteRule ^reservation/(.*)$ admin-reservation-view.php?reservationID=$1 [NC,L]
</IfModule>

